{
  "name": "preview-swagger.nvim",
  "version": "0.1.0",
  "bin": "./bin/server.js",
  "scripts": {
    "clean": "rimraf app/lib",
    "build": "tsc -p tsconfig.build.json;yarn next build;yarn next export",
    "watch": "tsc -w",
    "lint": "eslint './{bin,lib,src,tests}/**/*.{ts,tsx}'",
    "test-unit": "jest unit --runInBand --config jest.config.json",
    "lint-fix": "eslint --fix './{bin,lib,src,tests}/**/*.{ts,tsx}'",
    "prepare": "npm-run-all clean build"
  },
  "devDependencies": {
    "@types/express": "4.17.15",
    "@types/fs-extra": "9.0.13",
    "@types/jest": "29.2.4",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "18.11.17",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@types/socket.io": "3.0.1",
    "@types/source-map-support": "0.5.6",
    "@types/swagger-ui-react": "^4.11.0",
    "@typescript-eslint/eslint-plugin": "5.46.1",
    "@typescript-eslint/parser": "5.46.1",
    "babel-plugin-prismjs": "2.1.0",
    "dotenv-cli": "6.0.0",
    "eslint": "8.30.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "fs-extra": "11.1.0",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "lint-staged": "13.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.1",
    "ts-jest": "29.0.3",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "dependencies": {
    "@hozi-dev/content-css": "0.6.0",
    "@hozi-dev/markdown-to-html": "0.10.4",
    "child_process": "1.0.2",
    "express": "4.18.2",
    "gray-matter": "4.0.3",
    "js-yaml": "^4.1.0",
    "log4js": "6.7.1",
    "neovim": "4.10.1",
    "next": "13.0.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.57.0",
    "socket.io": "4.5.4",
    "socket.io-client": "4.5.4",
    "swagger-ui-react": "^4.15.5",
    "tocbot": "^4.19.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./{bin,lib,src,tests}/**/*.{ts,tsx}": [
      "yarn lint-fix",
      "yarn lint",
      "git add"
    ]
  }
}
